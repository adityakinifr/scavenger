# Copy this file to .env and fill in your actual Twilio credentials
# Get these values from your Twilio Console: https://console.twilio.com/

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# OpenAI Configuration (for natural language processing in scavenger hunt)
OPENAI_API_KEY=your_openai_api_key_here

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True
SECRET_KEY=your_secret_key_here

# Optional: Database URL (if using a database instead of in-memory storage)
# DATABASE_URL=your_database_url_here 